    <string name="adb_root_error_dlg_title">An Error Occurred</string>
    <string name="adb_root_success">Congratulations &#8212; Permission Granted!</string>
    <string name="adb_enable_hidden_api_text">
        &lt;b>Optional:&lt;/b> Permit GSam to access hidden APIs. This &lt;b>is not&lt;/b> required, but will provide more &lt;a href="http://blogger.gsamlabs.com/2019/12/enhanced-statistics-for-android-pie-and.html">detailed results&lt;/a>.&lt;br/>&lt;br/>
        &lt;b>WARNING:&lt;/b> This hidden API access is a system wide setting, so other apps can access hidden APIs as well as GSam.
    </string>
    <string name="adb_enable_hidden_api_text2">
        C:\\> adb shell settings put global %1$s 1&lt;br/>
        C:\\>
    </string>
